// 10개 입력받고 오름차순 정렬하기

Scanner sc = new Scanner(System.in);
		
			int[] a = new int[10] ;
			int n;
			
			for(int i = 0; i<10; i++) {
				System.out.printf("%d번째 수를 입력하세요 : ", i+1);		
				a[i] = sc.nextInt();
			}
			
			for(int j = 0; j < 9; j++) {
				for(int k=j+1;k<9;k++) {
					if(a[j]>a[k]) {
						n = a[j];
						a[j] = a[k];
						a[k] = n;
					}
						
				
				}
				System.out.printf("   %d  ", a[j]);	
						
			}


//
